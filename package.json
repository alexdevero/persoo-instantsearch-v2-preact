{
  "private": true,
  "name": "instantsearchjs-preact-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "browserslist": [
    "> 1%",
    "IE >= 9",
    "last 6 versions"
  ],
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-synacor": "3.0.5",
    "eslint-plugin-react": "7.18.3",
    "if-env": "1.0.4",
    "preact-cli": "3.0.0-rc.6"
  },
  "dependencies": {
    "algoliasearch": "4.0.3",
    "instantsearch.js": "4.2.0",
    "preact": "10.3.2",
    "preact-render-to-string": "5.1.4"
  }
}
