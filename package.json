{
  "private": true,
  "name": "instantsearchjs-preact-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --template public/index.html",
    "serve": "preact build --template public/index.html && preact serve",
    "dev": "preact watch --template public/index.html",
    "lint": "eslint src"
  },
  "browserslist": [
    "> 1%",
    "IE >= 9",
    "last 6 versions"
  ],
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "7.0.0",
    "eslint-config-synacor": "3.0.5",
    "eslint-plugin-react": "7.20.0",
    "if-env": "1.0.4",
    "preact-cli": "3.0.0-rc.6"
  },
  "dependencies": {
    "algoliasearch": "4.2.0",
    "instantsearch.js": "4.4.1",
    "mdn-polyfills": "5.20.0",
    "persoo-templates": "1.1.1",
    "preact": "10.4.1",
    "preact-render-to-string": "5.1.8",
    "promise-polyfill": "8.1.3",
    "unorm": "1.6.0"
  }
}
